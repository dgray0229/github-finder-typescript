    "react-test-renderer": "^16.13.1",
    "ts-jest": "^25.4.0"
  },
  "jest": {
    "moduleNameMapper": {
      "modulePaths": [
        "/shared/vendor/modules"
      ],
      "moduleFileExtensions": [
        "js",
        "jsx"
      ],
      "moduleDirectories": [
        "node_modules",
        "bower_components",
        "shared"
      ],
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.tsx",
